{
  "name": "@ghostwriter-qa/frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.8.1"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.8.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "vite build --config vite.config.demo.ts",
    "preview": "vite preview",
    "demo": "./start-demo.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:regression": "npm run test:e2e -- --grep 'Login Functionality'",
    "test:auth": "npm run test:e2e -- e2e/login.spec.ts",
    "test:projects": "npm run test:e2e -- e2e/projects.spec.ts",
    "test:api-import": "npm run test:e2e -- e2e/api-import.spec.ts",
    "test:generation": "npm run test:e2e -- e2e/test-generation.spec.ts",
    "test:environments": "npm run test:e2e -- e2e/environments.spec.ts",
    "test:execution": "npm run test:e2e -- e2e/test-execution.spec.ts",
    "test:smoke": "npm run test:e2e -- --grep '@smoke'",
    "test:critical": "npm run test:e2e -- --grep '@critical'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@monaco-editor/react": "^4.6.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.13.4",
    "@tanstack/react-query-devtools": "^5.80.5",
    "axios": "^1.6.2",
    "chart.js": "^4.4.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "framer-motion": "^10.17.0",
    "js-yaml": "^4.1.0",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "^0.45.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.8.1",
    "reactflow": "^11.10.1",
    "recharts": "^2.10.3",
    "socket.io-client": "^4.7.2",
    "swagger-parser": "^10.0.3",
    "uuid": "^9.0.1",
    "victory": "^36.7.0",
    "yup": "^1.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "wait-on": "^7.2.0"
  }
}